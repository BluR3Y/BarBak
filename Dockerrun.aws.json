{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "mysql",
      "image": "mysql:latest",
      "hostname": "mysql",
      "environment": [
        {
          "name": "MYSQL_ROOT_PASSWORD",
          "value": "Password@1234"
        }
      ],
      "essential": false,
      "memory": 300
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "hostname": "redis",
      "essential": false,
      "memory": 300
    },
    {
      "name": "backend",
      "image": "reyhec/barbak-backend:latest",
      "hostname": "backend",
      "essential": false,
      "memory": 300
    },
    {
      "name": "client",
      "image": "reyhec/barbak-client:latest",
      "hostname": "client",
      "essential": false,
      "memory": 300
    },
    {
      "name": "webserver",
      "image": "reyhec/barbak-webserver:latest",
      "hostname": "webserver",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "backend"],
      "memory": 100
    }
  ]
}