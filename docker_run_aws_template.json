{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "webserver",
        "image": "reyhec/barbak-webserver",
        "hostname": "webserver",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "backend"],
        "memory": 100
      },
      {
        "name": "client",
        "image": "reyhec/barbak-client",
        "hostname": "client",
        "essential": false,
        "memory": 300,
        "environmentFiles": [
          {
            "value": "arn:aws:s3:::barbak-application/env/client.env",
            "type": "s3"
          }
        ]
      },
      {
        "name": "backend",
        "image": "reyhec/barbak-backend",
        "hostname": "backend",
        "essential": false,
        "memory": 300,
        "environmentFiles": [
          {
            "value": "arn:aws:s3:::barbak-application/env/backend.env",
            "type": "s3"
          }
        ]
      },
      {
        "name": "mongo",
        "image": "reyhec/barbak-mongo",
        "hostname": "mongo",
        "essential": false,
        "memory": 300,
        "environmentFiles": [
          {
            "value": "arn:aws:s3:::barbak-application/env/mongo.env",
            "type": "s3"
          }
        ]
      },
      {
        "name": "mysql",
        "image": "reyhec/barbak-mysql",
        "hostname": "mysql",
        "essential": false,
        "memory": 300,
        "environmentFiles": [
          {
            "value": "arn:aws:s3:::barbak-application/env/mysql.env",
            "type": "s3"
          }
        ]
      },
      {
        "name": "redis",
        "image": "reyhec/barbak-redis",
        "hostname": "redis",
        "essential": false,
        "memory": 300
      }
    ]
}